package fr.ensma.a3.ia.mymultichat.api.message;

import fr.ensma.a3.ia.mymultichat.api.entities.User;

/**
 *
 * @author lammi
 */
public class ReadyToStartGameMessage extends ClientMessage {

    private final User user;

    public ReadyToStartGameMessage(int chanelId, User user) {
        super(chanelId);
        this.user = user;
    }

    public User getUser() {
        return user;
    }

}
